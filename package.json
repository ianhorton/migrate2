{
  "name": "sls-to-cdk-migrator",
  "version": "1.0.0",
  "description": "Automated Serverless Framework to AWS CDK migration tool",
  "main": "dist/index.js",
  "bin": {
    "sls-to-cdk": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "migrate": "ts-node src/cli/index.ts migrate",
    "scan": "ts-node src/cli/index.ts scan",
    "compare": "ts-node src/cli/index.ts compare",
    "generate": "ts-node src/cli/index.ts generate",
    "verify": "ts-node src/cli/index.ts verify",
    "rollback": "ts-node src/cli/index.ts rollback",
    "list": "ts-node src/cli/index.ts list",
    "status": "ts-node src/cli/index.ts status",
    "clean": "rm -rf dist .migration-state coverage",
    "prepublishOnly": "npm run build",
    "auth:codeartifact": "aws codeartifact login --tool npm --repository smart-packages --domain essensys-smart-packages --domain-owner 786267582114 --region eu-west-1",
    "preinstall": "command -v aws >/dev/null 2>&1 && npm run auth:codeartifact || echo 'AWS CLI not found, skipping CodeArtifact authentication'"
  },
  "keywords": [
    "serverless",
    "cdk",
    "migration",
    "aws",
    "cloudformation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.913.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.913.0",
    "@aws-sdk/client-dynamodb": "^3.913.0",
    "@aws-sdk/client-s3": "^3.913.0",
    "@aws-sdk/credential-provider-node": "^3.913.0",
    "@aws-sdk/credential-providers": "^3.913.0",
    "aws-sdk": "^2.1490.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "diff": "^5.1.0",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.7.0",
    "ora": "^5.4.1",
    "semver": "^7.5.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/diff": "^5.0.8",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
